{
	"manifest_version": 2,
	"name": "Cryptocurrency Historical Price Finder",
	"version": "0.1",
	

	"description" : "",
	
	"permissions": [
		"contextMenus"
	],
	
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["tooltipster.bundle.min.css", "tooltipster-light.min.css", "price_highlight.css"],
			"js": ["jquery-3.2.1.min.js","typeahead.bundle.min.js", "tooltipster.bundle.min.js","PriceHandler.js"]
		}
	],
	
	"background" : {
		"scripts": ["api_handler.js"]
	},
	
	"externally_connectable": {
		"ids": ["*"]
	},
	
	"web_accessible_resources": [
		"close.png",
		"hoverbox.html"
	]
	
}